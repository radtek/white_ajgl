<?xml version="1.0" encoding="utf-8"?>
<!-- ListView tree字段 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical"
	android:background="@color/btn_white_border"
	>
	<LinearLayout
				  android:layout_width="fill_parent"
				  android:layout_height="wrap_content"
				  android:orientation="vertical"
				  android:minHeight="40dip"
				  android:background="@color/btn_white_border"
				  android:id="@+id/case_item_p_lay"
		>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:layout_marginTop="5dp"
		android:layout_marginRight="5dp"
		android:layout_marginLeft="5dp"
		android:background="@color/btn_gray_border">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			android:background="@color/btn_white_border">
			<TextView
				android:id="@+id/case_item_in_list_item_name"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="手机"
				android:textColor="@color/onLine"
				android:layout_marginTop="8dp"
				android:textSize="13sp"
				android:maxEms="5"
				android:minEms="5"
				android:ellipsize="end"/>
			<com.taiji.androidui.view.viewHeightPlus.TextViewHeightPlus
				android:id="@+id/case_item_in_list_item_earmark"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="绿色VIVO"
				android:textColor="@color/black"
				android:layout_marginTop="8dp"
				android:textSize="13sp"
				android:layout_marginLeft="5dp"
				android:maxEms="12"
				android:minEms="12"
				android:layout_weight="1"
				android:ellipsize="end"/>
			<TextView
				android:id="@+id/case_item_in_list_item_count_showname"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="扣押:"
				android:textColor="@color/black"
				android:layout_marginTop="8dp"
				android:textSize="13sp"
				android:ellipsize="end"/>
			<TextView
				android:id="@+id/case_item_in_list_item_count"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="一部"
				android:textColor="@color/black"
				android:layout_marginTop="8dp"
				android:textSize="13sp"
				android:maxEms="4"
				android:minEms="4"
				android:ellipsize="end"/>

		</LinearLayout>


	</LinearLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:layout_marginTop="5dp"
		android:layout_marginRight="5dp"
		android:layout_marginLeft="5dp"
		android:background="@color/btn_gray_border">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			android:layout_weight="1"
			android:background="@color/btn_white_border">
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="1dp"
				android:orientation="horizontal"
				android:layout_weight="1"
				android:background="@color/btn_white_border">
				<ImageView
					android:id="@+id/case_item_in_list_item_code_iv"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="7dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/xs_icon_num" />
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="物品编号"
					android:textColor="@color/gray"
					android:layout_marginTop="5dp"
					android:textSize="12sp"
					android:maxEms="4"
					android:minEms="4"
					android:ellipsize="end"/>
				<com.taiji.androidui.view.viewHeightPlus.TextViewHeightPlus
					android:id="@+id/case_item_in_list_item_code_tv"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="2016000001"
					android:textColor="@color/gray"
					android:layout_marginTop="5dp"
					android:textSize="12sp"
					android:layout_marginLeft="5dp"
					android:maxEms="16"
					android:minEms="16"
					android:ellipsize="end"/>
			</LinearLayout>

			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="1dp"
				android:orientation="horizontal"

				android:background="@color/btn_white_border">
				<ImageView
					android:id="@+id/case_item_in_list_item_code_search_iv"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginRight="5dp"
					android:layout_gravity="center"
					android:src="@drawable/xs_icon_site"
					android:visibility="gone"
					/>
				<TextView
					android:id="@+id/case_item_in_list_item_docount_showname"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="已入库数量:"
					android:textColor="@color/gray"
					android:layout_marginTop="4dp"
					android:layout_marginBottom="4dp"
					android:layout_marginLeft="4dp"
					android:textSize="12sp"
					android:maxEms="5"
					android:minEms="2"
					android:ellipsize="end"/>
				<TextView
					android:id="@+id/case_item_in_list_item_docount_tv"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="0"
					android:textColor="@color/gray"
					android:layout_marginTop="4dp"
					android:layout_marginBottom="4dp"
					android:textSize="12sp"
					android:layout_marginLeft="0dp"
					android:maxEms="2"
					android:minEms="2"
					/>
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
			android:id="@+id/case_item_in_list_item_address_search_lay"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="0dp"
			android:orientation="horizontal"
			android:visibility="visible"
			android:background="@color/btn_white_border">
			<ImageView
				android:id="@+id/case_item_in_list_item_address_search_iv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="5dp"
				android:layout_gravity="center"
				android:visibility="gone"
				android:src="@drawable/xs_icon_site"
				/>
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text=" "
				android:textColor="@color/gray"
				android:layout_marginTop="4dp"
				android:layout_marginBottom="4dp"
				android:textSize="12sp"
				android:maxEms="6"
				android:minEms="2"
				android:ellipsize="end"/>
			<TextView
				android:id="@+id/case_item_in_list_item_address_tv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text=" "
				android:textColor="@color/gray"
				android:layout_marginTop="4dp"
				android:layout_marginBottom="4dp"
				android:textSize="12sp"
				android:layout_marginLeft="5dp"
				android:maxEms="16"
				android:minEms="2"
				android:layout_weight="1"
				android:ellipsize="end"/>
			<TextView
				android:id="@+id/case_item_in_list_item_new_tv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:visibility="visible"
				android:text="本次操作:"
				android:textColor="@color/onLine"
				android:layout_marginTop="4dp"
				android:layout_marginBottom="4dp"
				android:textSize="14sp"
				android:gravity="right"
				android:layout_marginRight="12dp"
				android:maxEms="6"
				android:minEms="3"
				android:ellipsize="end"/>
			<LinearLayout
				android:visibility="visible"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="#bcbcbc"
				android:orientation="vertical"
				android:layout_marginRight="12dp"
				android:focusable="true"
				android:focusableInTouchMode="true"
				>
				<com.taiji.androidui.view.MyEditText.FinishListenEditText
					android:id="@+id/case_item_in_list_item_new_ev"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:hint=""
					android:layout_marginTop="1dp"
					android:layout_marginBottom="1dp"
					android:layout_marginRight="1dp"
					android:layout_marginLeft="1dp"
					android:padding="4dp"
					android:text="0"
					android:background="#ffffff"
					android:textColor="#666666"
					android:textSize="12sp"
					android:visibility="visible"
					android:inputType="numberDecimal"
					android:gravity="right"
					android:maxEms="5"
					android:minEms="3"

					/>
			</LinearLayout>

		</LinearLayout>

	</LinearLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="3dp"
		android:background="@color/btn_gray_border">
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginBottom="1dp"
		android:visibility="visible"
		android:background="@color/btn_white_border">
	</LinearLayout>
	</LinearLayout>
</LinearLayout>
</LinearLayout>