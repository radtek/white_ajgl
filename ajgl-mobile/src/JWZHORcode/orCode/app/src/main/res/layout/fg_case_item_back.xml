<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f8f8f8">



    <LinearLayout
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:id="@+id/linearLayout">
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/btn_white_border">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/btn_white">
                <LinearLayout
                    android:id="@+id/case_item_info_lay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@color/background_blue">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="0.2dp"
                        android:layout_marginLeft="0.2dp"
                        android:layout_marginBottom="5dp">
                        <TextView
                            android:id="@+id/case_item_title_name_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="CK2016070001返还单"
                            android:textStyle="bold"
                            android:textColor="@color/share_text"
                            android:textSize="20sp"
                            android:singleLine="false"
                            android:autoText="false"
                            android:layout_marginRight="25dp"
                            android:layout_marginLeft="25dp"
                            android:layout_centerInParent="true"/>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:background="@color/btn_blue_border">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="1dp"
                                android:orientation="horizontal"
                                android:background="@color/background_blue">
                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text="返还时间"
                                    android:textColor="@color/gray"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                                <TextView
                                    android:id="@+id/case_item_time_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text="2014-01-26 18:00"
                                    android:textColor="@color/item_blue_text"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>


                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:background="@color/btn_blue_border">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="1dp"
                                android:orientation="vertical"
                                android:background="@color/background_blue">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text="对应借出单或其他类型单据编号"
                                    android:textColor="@color/gray"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                                <TextView
                                    android:id="@+id/case_item_orther_number_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text="AS530000001"
                                    android:textColor="@color/item_blue_text"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginLeft="100dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>


                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:background="@color/btn_blue_border">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="1dp"
                                android:orientation="horizontal"
                                android:background="@color/background_blue">
                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text="对应案件编号"
                                    android:textColor="@color/gray"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                                <TextView
                                    android:id="@+id/case_item_number_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text="AS530000001"
                                    android:textColor="@color/item_blue_text"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:background="@color/btn_blue_border">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="1dp"
                                android:orientation="horizontal"
                                android:background="@color/background_blue">
                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:text="案件名称"
                                    android:textColor="@color/gray"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                                <TextView
                                    android:id="@+id/case_item_name_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text="王某一,王某二,王某三非法持有毒品案"
                                    android:textColor="@color/item_blue_text"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginRight="15dp"
                            android:layout_marginLeft="15dp"
                            android:background="@color/background_blue">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:orientation="horizontal"
                                android:background="@color/background_blue">
                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:text="出库类型"
                                    android:textColor="@color/gray"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                                <TextView
                                    android:id="@+id/case_item_type_tv"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="随案移交"
                                    android:textColor="@color/item_blue_text"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginBottom="4dp"
                                    android:textSize="16sp"
                                    android:singleLine="false"
                                    android:autoText="false"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/case_item_item_lay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/btn_white"
        android:layout_below="@+id/linearLayout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/btn_gray_border">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="1dp"
                android:background="#f8f8f8">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:text="待返还库物品清单"
                    android:textColor="@color/gray"
                    android:textSize="16sp"
                    android:singleLine="false"
                    android:autoText="false"
                    android:layout_marginBottom="5dp"/>
            </RelativeLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="5dp"
            android:layout_weight="1">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="0dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/btn_white"
                    android:orientation="vertical"
                    android:layout_marginBottom="5dp">
                    <ListView
                        android:id="@+id/orderfeedback_detail_lv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="1dp"
                        android:layout_marginTop="1dp"
                        android:cacheColorHint="#00000000"
                        android:divider="@null"
                        android:textColor="#ffffff"
                        />
                </LinearLayout>
            </LinearLayout>
            <ImageView

                android:id="@+id/case_item_btn_next_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_gravity="center_vertical"
                android:scaleType="centerCrop"
                android:src="@drawable/btn_case_next" />


        </LinearLayout>


    </LinearLayout>



</RelativeLayout>